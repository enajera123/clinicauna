<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="parent" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="750.0" spacing="30.0" styleClass="bg-gray" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.clinicauna.controller.ReportModuleController">
   <children>
      <VBox>
         <children>
            <HBox spacing="5.0" styleClass="bg-black">
               <children>
                  <Button fx:id="btnDoctorReport" mnemonicParsing="false" onAction="#doctorReportAction" text="%doctorReport">
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="orange-color" />
                     </styleClass>
                  </Button>
                  <Button fx:id="btnMedicalExamReport" mnemonicParsing="false" onAction="#btnMedicalExamReportAction" text="%medicalExamReport">
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="orange-color" />
                     </styleClass>
                  </Button>
                  <Button fx:id="btnPatientReport" mnemonicParsing="false" onAction="#patientReportAction" text="%patientReport">
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="orange-color" />
                     </styleClass>
                  </Button>
                  <Button fx:id="btnReportGenerator" mnemonicParsing="false" onAction="#reportGeneratorAction" text="%reportGenerator">
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="orange-color" />
                     </styleClass>
                  </Button>
               </children>
            </HBox>
            <VBox fx:id="container" />
         </children></VBox>
   </children>
   <stylesheets>
      <URL value="@../style/index.css" />
      <URL value="@../style/reportmodule.css" />
   </stylesheets>
</VBox>
